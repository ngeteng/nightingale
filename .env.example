# URL RPC untuk jaringan Sepolia. Bisa didapat dari Alchemy, Infura, atau lainnya.
SEPOLIA_RPC_URL="https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID"

# Private key dari wallet AMAN yang akan kamu gunakan untuk mendeploy kontrak.
# Wallet ini harus punya saldo ETH Sepolia untuk membayar gas.
DEPLOYER_PRIVATE_KEY="YOUR_SAFE_WALLET_PRIVATE_KEY"

# Alamat wallet yang kuncinya bocor.
COMPROMISED_WALLET_ADDRESS="ADDRESS_OF_THE_COMPROMISED_WALLET"

# Alamat wallet AMAN milikmu sebagai tujuan akhir dana.
SECURE_WALLET_ADDRESS="YOUR_ULTIMATE_SECURE_WALLET_ADDRESS"
